{
    "name": "threeleaf/validation-engine",
    "description": "A Laravel Eloquent Model for creating, storing, and retrieving dynamic validation engine rules.",
    "version": "1.0.0",
    "type": "library",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "John A. Marsh",
            "email": "validation-engine@threeleafcom.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ThreeLeaf\\ValidationEngine\\": "src/",
            "Database\\Factories\\ThreeLeaf\\ValidationEngine\\Models\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^v10.48",
        "illuminate/support": "^10.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThreeLeaf\\ValidationEngine\\Providers\\ValidationServiceProvider"
            ]
        }
    }
}
